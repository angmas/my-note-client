
<ul>
{{#each notes as |note|}}

  <!-- {{> partial}}
  Don't uncomment yet -->

 <!-- Developers, why are we using data-id and not just id? -->
 <li class=note-container>
  <h1 data-id="{{note.id}}"> <small>{{note.id}} Title: {{note.title}}</small></h1>
  <div>
    {{#if note.description}}
      {{note.description}}
    {{else}}
      No description.
    {{/if}}
  </div>
  <button class="btn btn-primary btn-remove-class">Remove Note</button>
</li>

{{/each}}
</ul>
